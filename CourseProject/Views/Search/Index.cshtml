@using CourseProject_Models.ViewModels;
@model IEnumerable<Review>

<div class="container">
    <div class="row">
        @if (Model.Count() > 0 && Model != null)
        {
            @if (Model != null)
            {
                @foreach (var review in Model!)
                {
                    <partial name="_ShowReviews" model="review" />
                }
            }
        }
        else
        {
            <div>
                <p>@sharedResource["NothingFound"]</p>
            </div>
        }
    </div>
</div>